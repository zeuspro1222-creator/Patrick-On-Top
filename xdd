-- FAST FARMING SECTION
local FastFarm = Main:AddFolder("Fast Farming")

local farmSpeedValue = 2500
FastFarm:AddLabel("Velocidad actual: 2500")

FastFarm:AddTextBox("Farm Speed (1-2500)", function(text)
    local speed = tonumber(text)
    if speed and speed >= 1 and speed <= 2500 then
        farmSpeedValue = speed
    end
end)

local fastRepToggle = false
FastFarm:AddSwitch("Fast Strength", function(State)
    fastRepToggle = State
    if State then
        task.spawn(function()
            while fastRepToggle do
                for i = 1, farmSpeedValue do
                    if not fastRepToggle then break end
                    LocalPlayer:WaitForChild("muscleEvent"):FireServer("rep")
                end
                task.wait(0.1)
            end
        end)
    end
end)

FastFarm:AddLabel("‚ö†Ô∏è Mayor velocidad = Mayor lag")

-- üî• NUEVO: STRENGTH OP (HIGH/LOW PING) üî•
local StrengthOP = Main:AddFolder("‚ö° Strength OP (Phoenix)")

getgenv()._AutoRepFarmHighPing = false
getgenv()._AutoRepFarmLowPing = false

local function getPing()
    local success, ping = pcall(function()
        return Stats.Network.ServerStatsItem["Data Ping"]:GetValue()
    end)
    return success and ping or 999
end

local function equipPet(petName)
    local petsFolder = LocalPlayer:FindFirstChild("petsFolder")
    if petsFolder and petsFolder:FindFirstChild("Unique") then
        for _, pet in pairs(petsFolder.Unique:GetChildren()) do
            if pet.Name == petName then
                ReplicatedStorage.rEvents.equipPetEvent:FireServer("equipPet", pet)
                break
            end
        end
    end
end

local function hitRock(rockName)
    local rock = workspace:FindFirstChild(rockName)
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if rock and hrp then
        hrp.CFrame = rock.CFrame * CFrame.new(0, 0, -5)
        ReplicatedStorage.rEvents.hitEvent:FireServer("hit", rock)
    end
end

StrengthOP:AddSwitch("Strength Op (ping > 250ms)", function(state)
    getgenv()._AutoRepFarmHighPing = state
    
    if state then
        task.spawn(function()
            equipPet("Swift Samurai")
            local lastRock = tick()
            
            while getgenv()._AutoRepFarmHighPing do
                if LocalPlayer:FindFirstChild("muscleEvent") then
                    for i = 1, 10 do
                        LocalPlayer.muscleEvent:FireServer("rep")
                    end
                end
                
                if tick() - lastRock >= 5 then
                    hitRock("Rock5M")
                    lastRock = tick()
                end
                
                task.wait(0.01)
            end
        end)
    end
end)

StrengthOP:AddSwitch("Strength Op (ping < 150ms)", function(state)
    getgenv()._AutoRepFarmLowPing = state
    
    if state then
        task.spawn(function()
            equipPet("Swift Samurai")
            local lastRock = tick()
            
            while getgenv()._AutoRepFarmLowPing do
                if LocalPlayer:FindFirstChild("muscleEvent") then
                    for i = 1, 40 do
                        LocalPlayer.muscleEvent:FireServer("rep")
                    end
                end
                
                if tick() - lastRock >= 5 then
                    hitRock("Rock5M")
                    lastRock = tick()
                end
                
                task.wait(0.01)
            end
        end)
    end
end)

StrengthOP:AddLabel("Equipa Swift Samurai antes")

