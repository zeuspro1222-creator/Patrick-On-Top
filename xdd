-- OPTIMIZACI√ìN SECTION
local OptSection = Main:AddFolder("‚ö° Optimizaci√≥n")

OptSection:AddButton("Anti Lag", function()
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
            v.Enabled = false
        end
    end
    
    local lighting = game:GetService("Lighting")
    lighting.GlobalShadows = false
    lighting.FogEnd = 9e9
    lighting.Brightness = 0
    settings().Rendering.QualityLevel = 1
    
    print("‚úÖ Anti-Lag activado")
end)

OptSection:AddButton("Full Optimization", function()
    local lighting = game:GetService("Lighting")
    
    -- Eliminar cielo
    for _, v in pairs(lighting:GetChildren()) do
        if v:IsA("Sky") then v:Destroy() end
    end
    
    -- Cielo oscuro
    local darkSky = Instance.new("Sky")
    darkSky.SkyboxBk = "rbxassetid://0"
    darkSky.SkyboxDn = "rbxassetid://0"
    darkSky.SkyboxFt = "rbxassetid://0"
    darkSky.SkyboxLf = "rbxassetid://0"
    darkSky.SkyboxRt = "rbxassetid://0"
    darkSky.SkyboxUp = "rbxassetid://0"
    darkSky.Parent = lighting
    
    lighting.Brightness = 0
    lighting.ClockTime = 0
    lighting.OutdoorAmbient = Color3.new(0, 0, 0)
    
    -- Eliminar part√≠culas
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("ParticleEmitter") then
            obj:Destroy()
        end
    end
    
    print("‚úÖ Full Optimization activada")
end)

OptSection:AddButton("Hide All Frames", function()
    local rSto = ReplicatedStorage
    for _, obj in pairs(rSto:GetChildren()) do
        if obj.Name:match("Frame$") then
            obj.Visible = false
        end
    end
end)

-- MASCOTAS SECTION
local PetSection = Main:AddFolder("üêæ Mascotas")

PetSection:AddButton("Equip Swift Samurai (8x)", function()
    local petsFolder = LocalPlayer:FindFirstChild("petsFolder")
    if not petsFolder then return end

    -- Desequipar todo
    for _, folder in pairs(petsFolder:GetChildren()) do
        for _, pet in pairs(folder:GetChildren()) do
            ReplicatedStorage.rEvents.equipPetEvent:FireServer("unequipPet", pet)
        end
    end
    
    task.wait(0.2)

    -- Equipar Swift Samurai
    local count = 0
    for _, folder in pairs(petsFolder:GetChildren()) do
        for _, pet in pairs(folder:GetChildren()) do
            if pet.Name == "Swift Samurai" and count < 8 then
                ReplicatedStorage.rEvents.equipPetEvent:FireServer("equipPet", pet)
                count = count + 1
                task.wait(0.1)
            end
        end
    end
    
    print("‚úÖ Equipados " .. count .. " Swift Samurai")
end)

PetSection:AddButton("Equip Tribal Overlord (8x)", function()
    local petsFolder = LocalPlayer:FindFirstChild("petsFolder")
    if not petsFolder then return end

    -- Desequipar todo
    for _, folder in pairs(petsFolder:GetChildren()) do
        for _, pet in pairs(folder:GetChildren()) do
            ReplicatedStorage.rEvents.equipPetEvent:FireServer("unequipPet", pet)
        end
    end
    
    task.wait(0.2)

    -- Equipar Tribal Overlord
    local count = 0
    for _, folder in pairs(petsFolder:GetChildren()) do
        for _, pet in pairs(folder:GetChildren()) do
            if pet.Name == "Tribal Overlord" and count < 8 then
                ReplicatedStorage.rEvents.equipPetEvent:FireServer("equipPet", pet)
                count = count + 1
                task.wait(0.1)
            end
        end
    end
    
    print("‚úÖ Equipados " .. count .. " Tribal Overlord")
end)

