    local args = {"changeSize", 0/0}
    ReplicatedStorage:WaitForChild("rEvents"):WaitForChild("changeSpeedSizeRemote"):InvokeServer(unpack(args))
end)

-- ========================================
-- STATS TAB
-- ========================================
local StatsTab = window:AddTab("Stats", Color3.fromRGB(128, 0, 128))

local function abbreviateNumber(value)
    if value >= 1e15 then
        return string.format("%.1fQa", value / 1e15)
    elseif value >= 1e12 then
        return string.format("%.1fT", value / 1e12)
    elseif value >= 1e9 then
        return string.format("%.1fB", value / 1e9)
    elseif value >= 1e6 then
        return string.format("%.1fM", value / 1e6)
    elseif value >= 1e3 then
        return string.format("%.1fK", value / 1e3)
    else
        return tostring(math.floor(value))
    end
end

local timeLabel = StatsTab:AddLabel("Tiempo en servidor: Calculando...")
local strengthLabel = StatsTab:AddLabel("Fuerza ganada: 0")
local durabilityLabel = StatsTab:AddLabel("Durabilidad ganada: 0")
local agilityLabel = StatsTab:AddLabel("Agilidad ganada: 0")
local killsLabel = StatsTab:AddLabel("Kills ganados: 0")
local rebirthsLabel = StatsTab:AddLabel("Rebirths ganados: 0")

local startTime = tick()
local initialStats = {
    strength = LocalPlayer.leaderstats.Strength.Value,
    rebirths = LocalPlayer.leaderstats.Rebirths.Value,
    durability = LocalPlayer.Durability.Value,
    agility = LocalPlayer.Agility.Value,
    kills = LocalPlayer.leaderstats.Kills.Value
}

task.spawn(function()
    while true do
        local elapsed = tick() - startTime
        local hours = math.floor(elapsed / 3600)
        local minutes = math.floor((elapsed % 3600) / 60)
        local seconds = math.floor(elapsed % 60)
        
        timeLabel.Text = string.format("Tiempo: %dh %dm %ds", hours, minutes, seconds)
        
        local currentStrength = LocalPlayer.leaderstats.Strength.Value
        local currentRebirths = LocalPlayer.leaderstats.Rebirths.Value
        local currentDurability = LocalPlayer.Durability.Value
        local currentAgility = LocalPlayer.Agility.Value
        local currentKills = LocalPlayer.leaderstats.Kills.Value
        
        strengthLabel.Text = "Fuerza ganada: " .. abbreviateNumber(currentStrength - initialStats.strength)
        rebirthsLabel.Text = "Rebirths ganados: " .. abbreviateNumber(currentRebirths - initialStats.rebirths)
        durabilityLabel.Text = "Durabilidad ganada: " .. abbreviateNumber(currentDurability - initialStats.durability)
        agilityLabel.Text = "Agilidad ganada: " .. abbreviateNumber(currentAgility - initialStats.agility)
        killsLabel.Text = "Kills ganados: " .. abbreviateNumber(currentKills - initialStats.kills)
        
        task.wait(1)
    end
end)

-- ========================================
-- PLAYER TAB
-- ========================================
local Player = window:AddTab("Player", Color3.fromRGB(128, 0, 128))

local walkSpeedValue = 16
Player:AddTextBox("WalkSpeed", function(text)
    local speed = tonumber(text)
    if speed and speed >= 1 and speed <= 500 then
        walkSpeedValue = speed
    end
end)

local setSpeed = false
Player:AddSwitch("Set Speed", function(state)
    setSpeed = state
    if state then
        task.spawn(function()
            while setSpeed do
                pcall(function()
                    LocalPlayer.Character.Humanoid.WalkSpeed = walkSpeedValue
                end)
                task.wait(0.1)
            end
        end)
    else
        pcall(function()
            LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end)
    end
end)

local jumpPowerValue = 50
Player:AddTextBox("JumpPower", function(text)
    local jump = tonumber(text)
    if jump then
        jumpPowerValue = jump
    end
end)

local applyJumpPower = false
Player:AddSwitch("Apply JumpPower", function(state)
    applyJumpPower = state
    pcall(function()
        LocalPlayer.Character.Humanoid.UseJumpPower = true
        if applyJumpPower then
            LocalPlayer.Character.Humanoid.JumpPower = jumpPowerValue
        else
            LocalPlayer.Character.Humanoid.JumpPower = 50
        end
    end)
end)

